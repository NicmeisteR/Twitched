{
    "manifest_version": 2,
    "name": "Twitched",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": [
          "https://www.twitch.tv/*"
        ],
        "js": ["jquery.js", "content.js"],
        "css": ["style.css"]
      }
    ],
    "browser_action": {
        "default_icon": "logo.png"
    }
  }